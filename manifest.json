{
  "manifest_version": 3,
  "name": "Uner",
  "version": "1.0",
  "description": "Toggle cleaning of tracking parameters from URLs on any page.",
  "permissions": ["scripting", "activeTab", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
